<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    Android 样式：常用主题属性 -->
<!--    colorPrimary：应用的主要品牌颜色，最常用于主题 toolbar-->
<!--    colorPrimaryVariant：您的主要品牌颜色的较浅/较深变体，在主题中很少使用 状态栏-->
<!--    colorOnPrimary：用于显示在原色之上的元素的颜色（例如，文本和图标，通常为白色或半- 透明黑色，具体取决于可访问性）-->
<!--    colorSecondary：您的应用的辅助品牌颜色，主要用作某些需要突出的小部件的重点-->
<!--    colorSecondaryVariant：您的辅助品牌颜色的较浅/较深变体，在主题中很少使用-->
<!--    colorOnSecondary：用于显示在辅助颜色之上的元素的颜色-->
<!--    colorError：用于错误的颜色（通常是红色阴影）-->
<!--    colorOnError：用于显示在错误颜色之上的元素的颜色-->
<!--    colorSurface：用于表面的颜色（即材料“片材”）-->
<!--    colorOnSurface：用于显示在表面颜色之上的元素的颜色-->
<!--    colorBackground：所有其他屏幕内容背后的颜色-->
<!--    colorOnBackground：用于显示在背景颜色之上的元素的颜色-->


    <!-- 自定义主题颜色 -->
    <!--    colorPrimary：您的应用的主要品牌颜色，主要用于主题-->
    <color name="brand">@color/colorPrimary</color>
    <!--    colorPrimary：您的应用的主要品牌颜色，主要用于主题-->
    <color name="colorPrimary">#14e0d0</color>
    <color name="colorPrimaryDark">@color/deep_blue</color>
    <color name="colorAccent">@color/colorPrimary</color>
    <color name="colorPrimaryVariant">#512DA8</color>
    <color name="colorOnPrimary">@color/white</color>
    <color name="colorSecondary">@color/light_blue</color>
    <color name="colorSecondaryVariant">#0039CB</color>
    <color name="colorOnSecondary">#212121</color>
    <color name="colorError">#ff5454</color>
    <color name="colorOnError">#FFFFFF</color>
    <color name="colorSurface">@color/light_white</color>
    <color name="colorOnSurface">#212121</color>
    <color name="colorBackground">@color/grey100</color>
    <color name="colorOnBackground">@color/white</color>

    <!--    比背景色深一点-->
    <color name="windowBackground">@color/grey150</color>
    <color name="windowBackgroundDark">@color/grey200</color>
    <color name="textColorPrimary">@color/color_text_dark</color>

    <color name="line_divider">@color/grey400</color>
    <color name="mask_color">#f5f5f5</color>

    <!-- color -->
    <color name="color_text_light">@color/grey200</color>
    <color name="color_text_dark">#111111</color>
    <color name="color_text_normal">@color/grey700</color>
    <color name="color_text_gray">@color/grey600</color>


    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>



    <color name="grey50">#FaFaFa</color>
    <color name="grey100">#F5F5F5</color>
    <color name="grey150">#F0F0F0</color>
    <color name="grey200">#EEEEEE</color>
    <color name="grey250">#E8E8E8</color>
    <color name="grey300">#E0E0E0</color>
    <color name="grey400">#bdbdbd</color>
    <color name="grey500">#9e9e9e</color>
    <color name="grey600">#757575</color>
    <color name="grey700">#616161</color>
    <color name="grey800">#424242</color>
    <color name="grey900">#ff2C2C2C</color>
</resources>
